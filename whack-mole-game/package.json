{
  "name": "whack-mole-game",
  "version": "1.0.0",
  "description": "AI Character Puzzle Whack-a-Mole Game - A browser-based game where players click moles to collect puzzle pieces that form AI-generated animated characters",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:unit": "vitest run tests/unit",
    "test:component": "vitest run tests/component",
    "test:e2e": "playwright test",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .js,.vue",
    "lint:fix": "eslint src --ext .js,.vue --fix"
  },
  "dependencies": {
    "pinia": "^2.1.7",
    "vue": "^3.4.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.0",
    "@vitest/coverage-v8": "^1.1.0",
    "@vue/test-utils": "^2.4.0",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.19.0",
    "jsdom": "^27.4.0",
    "playwright": "^1.40.0",
    "vite": "^5.0.0",
    "vitest": "^1.1.0"
  },
  "keywords": [
    "game",
    "whack-a-mole",
    "puzzle",
    "ai",
    "vue",
    "vite"
  ],
  "author": "",
  "license": "MIT"
}
